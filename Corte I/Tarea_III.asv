clc
clear
close all


f = 100 % frecuencia
T = 1/f % periodo


Ts = T/10 % unidad de tiempo
t = -2*T:Ts:2*T; % tiempo/frecuencia
y = abs(t); % funcion


figure(1)
subplot(1, 2, 1)
plot(t, y, 'g')
title("Señal Continua x_{(t)} = |t|")
xlabel("Tiempo (s)")
ylabel("Amplitud")
grid on

